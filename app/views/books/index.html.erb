<h1>Books</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Series</th>
    <th>ISBN</th>
    <th>Genre</th>
    <th>Description</th>
  </tr>

  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= book.series %></td>
      <td><%= book.isbn %></td>
      <td><%= book.genre %></td>
      <td><%= book.description %></td>
    </tr>
  <% end %>
</table>
